{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "dev": "nuxt dev",
    "generate:kda:types": "npx pactjs contract-generate --contract=free.crowdfund --api=https://api.testnet.chainweb.com/chainweb/0.0/testnet04/chain/0/pact",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "db:reset": "npx supabase db reset && yarn db:types",
    "db:types": "supabase gen types typescript --local > ./supabase/schema.ts",
    "kadena:devnet": "docker run -it -p 8080:1337 -v $HOME/.devnet/l1:/root/.devenv enof/devnet",
    "kadena:cli": "docker run --rm -it -v $(pwd):/app/ --add-host=devnet:host-gateway enof/kda-cli",
    "kadena:types": "npx pactjs contract-generate --contract=free.crowdfund --api=https://api.testnet.chainweb.com/chainweb/0.0/testnet04/chain/0/pact",
    "kadena:fund": "yarn kadena:cli --profile=.fund-profile.json",
    "kadena:deploy": "yarn kadena:cli --profile=.deploy-profile.json",
    "wdio:component": "wdio run ./tests/wdio.comp.conf.ts",
    "wdio:e2e": "wdio run ./tests/wdio.e2e.conf.ts",
    "wdio:sauce": "wdio run ./tests/wdio.sauce.conf.ts"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@nuxt/devtools": "latest",
    "@nuxtjs/supabase": "^0.3.8",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/vue": "^7.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/test-utils": "^2.4.1",
    "@wdio/browser-runner": "^8.13.13",
    "@wdio/cli": "^8.13.13",
    "@wdio/mocha-framework": "^8.13.13",
    "@wdio/sauce-service": "^8.13.13",
    "@wdio/spec-reporter": "^8.12.2",
    "chromedriver": "^115.0.0",
    "nuxt": "^3.6.3",
    "supabase": "^1.77.9",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "wdio-chromedriver-service": "^8.1.1",
    "wdio-nuxt-service": "^0.1.0"
  },
  "dependencies": {
    "@kadena/client": "1.0.0-alpha.8",
    "@kadena/pactjs": "^0.3.0",
    "@kadena/pactjs-cli": "1.0.0-alpha.7",
    "@nuxtjs/tailwindcss": "^6.8.0",
    "@vee-validate/nuxt": "^4.10.7",
    "@vee-validate/zod": "^4.10.7",
    "@vueuse/core": "^10.2.1",
    "@vueuse/nuxt": "^10.2.1",
    "@walletconnect/encoding": "^1.0.2",
    "@walletconnect/modal": "^2.5.9",
    "@walletconnect/sign-client": "2.8.6",
    "@walletconnect/types": "2.8.6",
    "autoprefixer": "^10.4.14",
    "daisyui": "^3.1.7",
    "dotenv": "^16.3.1",
    "lokijs": "^1.5.12",
    "nanoid": "^4.0.2",
    "nuxt-proxy": "^0.4.1",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "vee-validate": "^4.10.7",
    "viem": "^1.3.0",
    "zod": "^3.21.4"
  }
}
